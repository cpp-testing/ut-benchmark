#define BOOST_TEST_MODULE SuiteAssertModule
#include <boost/test/unit_test.hpp>

struct SuiteAssert {};

<% (1..n).each do |i| %>
  BOOST_FIXTURE_TEST_CASE(test_<%= v %><%= i %>) {
    BOOST_CHECK(true == true);
    BOOST_CHECK(42 == 42);
    BOOST_CHECK(1 != 2);
    BOOST_CHECK(1.23f == 1.23);
  }
<% end %>
