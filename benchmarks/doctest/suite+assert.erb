#define DOCTEST_CONFIG_IMPLEMENTATION_IN_DLL
#define DOCTEST_CONFIG_SUPER_FAST_ASSERTS
#include <doctest.h>

TEST_CASE("suite+assert.<%= v %>") {
  <% (1..n).each do |i| %>
    SUBCASE("<%= v %>.<%= i %>") {
      CHECK(true == true);
      CHECK(42 == 42);
      CHECK(1 != 2);
      CHECK(1.23f == doctest::Approx(1.23));
    }
  <% end %>
}
