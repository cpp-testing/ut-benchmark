#define DOCTEST_CONFIG_IMPLEMENTATION_IN_DLL
#define DOCTEST_CONFIG_SUPER_FAST_ASSERTS
#include <doctest.h>
#include <string>
#include <vector>

TEST_CASE("suite+assert+stl.<%= v %>") {
  <% (1..n).each do |i| %>
    SUBCASE("<%= v %>.<%= i %>") {
      CHECK(std::string{"pass"} == std::string{"<%= match %>"});
      CHECK(std::vector{1, 2, 3} == std::vector{1, 2, 3});
    }
  <% end %>
}
