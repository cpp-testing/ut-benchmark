#include <boost/ut.hpp>
#include <string>
#include <vector>

namespace ut = boost::ut;

static ut::suite _ = [] {
  using namespace ut;

  <% (1..n).each do |i| %>
    "test_suite_<%= v %>.<%= i %>"_test = [] {
      expect(std::string{"pass"} == std::string{"<%= match %>"});
      expect(std::vector{1, 2, 3} == std::vector{1, 2, 3});
		};
  <% end %>
};
