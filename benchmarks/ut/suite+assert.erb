#define BOOST_UT_INTERFACE
#include <boost/ut.hpp>

namespace ut = boost::ut;

static ut::suite _ = [] {
  using namespace ut;

  <% (1..n).each do |i| %>
    "test_suite_<%= v %>.<%= i %>"_test = [] {
      expect(_b(true) == true);
      expect(42_i == 42);
      expect(1 != 2);
      expect(1.23f == 1.23_f);
		};
  <% end %>
};
